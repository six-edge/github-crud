{
  "name": "mr-crud",
  "version": "0.1.0",
  "homepage": "https://mr-crud.herokuapp.com/",
  "description": "Simple App that uses the GitHub API to CRUD files. Based on Node.js, Express, Vue.js and Vuetify",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service build --mode dev --watch",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node src/node/server.js",
    "watch": "nodemon src/node/server.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "connect-redis": "^3.4.0",
    "dotenv": "^6.2.0",
    "express-session": "^1.15.6",
    "global": "^4.3.2",
    "ip": "^1.1.5",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "vue": "^2.6.8",
    "vue-native-websocket": "^2.0.13",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-plugin-vue": "^5.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.8",
    "vuetify-loader": "^1.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
